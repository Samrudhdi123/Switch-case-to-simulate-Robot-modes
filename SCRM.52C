#include <stdio.h>

int main() {
    int mode;
    printf("Select GPS Mode (1=Manual, 2=Auto, 3=Tracking): ");
    scanf("%d", &mode);

    switch (mode) {
        case 1: printf("Manual GPS Mode Activated.\n"); break;
        case 2: printf(" Auto GPS Mode Activated.\n"); break;
        case 3: printf("Tracking Mode On.\n"); break;
        default: printf("Invalid Mode.\n");
    }

    return 0;
}
