#include <stdio.h>

int main() {
    int battery;
    printf("Enter battery percentage: ");
    scanf("%d", &battery);

    if (battery < 20)
        printf("Charging Started...\n");
    else if (battery < 80)
        printf(" Continue Charging...\n");
    else
        printf(" Battery Fully Charged.\n");

    return 0;
}
